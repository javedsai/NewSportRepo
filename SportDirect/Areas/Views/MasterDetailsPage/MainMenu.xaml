        <pages:MainMenuMaster Title="Home" x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages1:MainTabbedBar  />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
  </MasterDetailPage>      